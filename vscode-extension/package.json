{
  "name": "quarkscript",
  "displayName": "QuarkScript",
  "description": "vscode extension for my esolang quarkscript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "type": "module",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "quarkscript",
        "aliases": [
          "QuarkScript",
          "qs"
        ],
        "extensions": [
          ".qs",
          ".quarkscript"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "quarkscript",
        "scopeName": "source.quarkscript",
        "path": "./syntaxes/qs.tmLanguage.json"
      }
    ],
    "completionItems": [
      {
        "language": "quarkscript",
        "path": "./completions/quarkscriptCompletion.json"
      }
    ],
    "snippets": [
      {
        "language": "quarkscript",
        "path": "./snippets/qs.json"
      }
    ],
    "commands": []
  },
  "main": "src/index.js",
  "activationEvents": [
    "onLanguage:quarkscript"
  ],
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "author": "xShadowBlade",
  "license": "MIT",
  "dependencies": {
    "@types/vscode": "^1.83.1"
  }
}
